{
    "name": "ampluso/pinger-bundle",
    "type": "symfony-bundle",
    "description": "PingerBundle (bundle for Symfony2) is a service to update different search engines that your blog or website has updated.",
    "keywords": ["pinger", "bundle", "google", "service", "ping", "xml-rpc", "extended ping"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Chylek",
            "email": "marcin@chylek.pl",
            "homepage": "http://ampluso.pl"
        }
    ],
    "support": {
        "issues": "http://github.com/ampluso/PingerBundle/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "2.*"
    },
    "autoload": {
        "psr-0": { "Ampluso\\PingerBundle": "" }
    },
    "target-dir": "Ampluso/PingerBundle",
    "minimum-stability": "dev"
}